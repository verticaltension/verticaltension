\par\plain \ilvl0{\listtext	\f0\fs24\b0\i0 \uc0\u8226 	}{\f0\fs24\b0\i0 Enhancement without discipline}
\par\pard\plain \sa180\sb180\ltrch\loch {\f0\fs24\b0\i0 Instead, he aligns biology with }{\f0\fs24\b1\i0 teleological evolution}{\f0\fs24\b0\i0 : every act of preservation must serve }{\f0\fs24\b1\i0 directional becoming}{\f0\fs24\b0\i0 .}
