\par\pard\plain \sa180\sb210\ltrch\loch {\f0\fs28\b1\i0 9. The Eco-\loch\af0\hich\af0\dbch\af0\uc1\u220\'DCbermensch: Manifesto of Becoming}
\par\pard\plain \sa180\sb180\ltrch\loch {\f0\fs24\b0\i0 \loch\af0\hich\af0\dbch\af0\uc1\u8220\'93I do not protect nature.\line I evolve with it.\line I do not worship the Earth.\line I sculpt her.\line I am not her child.\line I am her future.\u8221\'94}
\par\plain {\f0\fs24\b0\i0 Vertical Biocentrism is not \loch\af0\hich\af0\dbch\af0\uc1\u8220\'93green.\u8221\'94\line It is }{\f0\fs24\b1\i0 gold}{\f0\fs24\b0\i0 \hich\f0 \emdash \loch\f0 solar, radiant, ascending.}
\par\plain {\f0\fs24\b0\i0 It is not conservation.\line It is }{\f0\fs24\b1\i0 creation through biological refinement}{\f0\fs24\b0\i0 .}
\par\plain {\f0\fs24\b0\i0 The Overman becomes the }{\f0\fs24\b1\i0 genetic poet of the planet}{\f0\fs24\b0\i0 .\line His body, his state, his art, and his ecosystem form a }{\f0\fs24\b1\i0 single unified will}{\f0\fs24\b0\i0 .}
